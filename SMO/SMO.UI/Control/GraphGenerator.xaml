<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
	mc:Ignorable="d"
	x:Class="SMO.UI.Control.GraphGenerator"
	x:Name="UserControl"
	d:DesignWidth="640" Height="738.5">

	<Grid x:Name="LayoutRoot">
		<d3:ChartPlotter d:LayoutOverrides="Width" Height="382.5" VerticalAlignment="Top" x:Name="plotterInterval"/>
		<d3:ChartPlotter d:LayoutOverrides="Width" Height="351" VerticalAlignment="Bottom"/>
	</Grid>
</UserControl>